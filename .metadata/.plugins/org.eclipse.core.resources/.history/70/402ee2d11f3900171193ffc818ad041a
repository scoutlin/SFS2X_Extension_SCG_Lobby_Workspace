package scott.scg.loginVerify;

public class LoginVerify
{
	public static LoginVerify mLoginVerify;
	
	
	
	public LoginVerify()
	{
		
	}
	
	
	
	public static LoginVerify GetInstance()
	{
		if(mLoginVerify == null)
		{
			mLoginVerify = new LoginVerify();
			return mLoginVerify;
		}
		else
		{
			return mLoginVerify;
		}
	}

}
